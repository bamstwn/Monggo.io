{
  "name": "monggo-io",
  "version": "1.0.0",
  "private": true,
  "description": "Monggo IO: The web app that allows users to make hotel requests without worry.",
  "keywords": [
    "nuxt",
    "nuxt pwa",
    "nuxt.js",
    "nuxtjs",
    "vue",
    "vue pwa",
    "vue ssr",
    "vue universal",
    "vue.js",
    "vuejs",
    "vuetify",
    "vuetify.js",
    "vuetifyjs"
  ],
  "homepage": "https://github.com/jefrydco/nuxt-pwa-vuetify-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jefrydco/nuxt-pwa-vuetify-starter"
  },
  "license": "MIT",
  "author": {
    "name": "Jefry Dewangga",
    "url": "https://jefrydco.id"
  },
  "scripts": {
    "build": "nuxt build",
    "coverage": "codecov",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "start": "nuxt start",
    "test": "npm run test:unit",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:unit": "jest",
    "build:firebase": "yarn --verbose --ignore-engines && yarn clean && yarn build && yarn copy && cd \"functions\" && yarn --verbose --ignore-engines",
    "clean": "yarn clean:public && yarn clean:functions && yarn clean:static",
    "clean:functions": "rimraf \"functions/node_modules\" && rimraf \"functions/.nuxt\"",
    "clean:public": "rimraf -rf \"public/**/*.!(md)\" && rimraf -rf \"public/_nuxt\"",
    "clean:static": "rimraf \"src/static/sw.js\"",
    "copy": "cpx \"functions/.nuxt/dist/**/*\" \"public/_nuxt\" && cpx \"src/static/**/*.!(md)\" \"public\"",
    "deploy": "firebase deploy --only functions,hosting --debug",
    "start:firebase": "firebase serve --only functions,hosting"
  },
  "dependencies": {
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/moment": "^1.2.0",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "@nuxtjs/sentry": "^3.0.0",
    "cookie-universal-nuxt": "^2.0.17",
    "nuxt": "^2.3.4",
    "nuxt-i18n": "^6.0.1",
    "vee-validate": "^2.1.5",
    "vue-qrcode-reader": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/vuetify": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.28",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "codecov": "^3.1.0",
    "cpx": "^1.5.0",
    "cypress": "^3.1.4",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-nuxt": "^0.4.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.1.0",
    "eslint-plugin-vuetify": "^1.0.0-beta.3",
    "husky": "^2.3.0",
    "jest": "^24.0.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "vue-jest": "^3.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
